{
    "manifest_version": 3,
    "name": "LinkedIn Profile Extension",
    "description": "Get Profile Detail of LinkedIn",
    "version": "2.0",
    "action": {
        "default_icon": "logo.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.linkedin.com/in/*"
            ],
            "js": [
                "extension.js"
            ]
        }
    ]
}